function Others()
    TabFrames:WaitForChild("Others").UIGridLayout:Destroy()
    local Title = Instance.new("TextLabel")
    local TextLabel = Instance.new("TextLabel")
    local UIListLayout = Instance.new("UIListLayout")

    UIListLayout.Parent = TabFrames:WaitForChild("Others")
    UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder 

    Title.Name = "Title"
    Title.Parent = TabFrames:WaitForChild("Others")
    Title.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    Title.BackgroundTransparency = 1.000
    Title.Size = UDim2.new(1, 0, 0.0403994955, 0)
    Title.Font = Enum.Font.SourceSans
    Title.Text = "Credits to"
    Title.TextColor3 = Color3.fromRGB(255, 255, 255)
    Title.TextScaled = true
    Title.TextSize = 14.000
    Title.TextStrokeColor3 = Color3.fromRGB(255, 255, 255)
    Title.TextWrapped = true

    TextLabel.Parent = TabFrames:WaitForChild("Others")
    TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    TextLabel.BackgroundTransparency = 1.000
    TextLabel.Size = UDim2.new(1, 0, 0.0403994955, 0)
    TextLabel.Font = Enum.Font.SourceSans
